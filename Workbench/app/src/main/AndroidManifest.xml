<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="my.android.workbench">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Example2Activity"
            android:label="@string/title_activity_example2"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="my.android.workbench.MainActivity" />
        </activity>
        <activity
            android:name=".AppSlowResponseActivity"
            android:label="@string/title_activity_app_no_response"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="my.android.workbench.MainActivity" />
        </activity>
        <activity
            android:name=".AppSlowResponseAsyncActivity"
            android:label="@string/title_activity_app_slow_response_async"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="my.android.workbench.MainActivity" />
        </activity>
        <activity
            android:name=".ListExampleActivity"
            android:label="@string/title_activity_list_example"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="my.android.workbench.MainActivity" />
        </activity>
    </application>

</manifest>